  "description": "A Javascript library for traversing object trees.",
  "main": "src/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
